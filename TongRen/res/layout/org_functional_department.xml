<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:edittextview="http://schemas.android.com/apk/res/com.tr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f1f1"
    android:orientation="vertical" >
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:background="#22292c" >

        <RelativeLayout
            android:id="@+id/quit_function_Rl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="11dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:background="@drawable/people_left_arrow"
                android:focusable="false" />
        </RelativeLayout>

        <TextView
            android:id="@+id/org_function_Tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="主要职能部门"
            android:textColor="#ffffff"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/finishs_function_Tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:clickable="true"
            android:enabled="true"
            android:padding="10dp"
            android:onClick="finish"
            android:textColor="#fff"
            android:text="完成" />
    </RelativeLayout>
    <ScrollView 
        android:background="#f1f1f1"
        android:layout_width="match_parent"
            android:layout_height="match_parent">
     <LinearLayout 
         android:id="@+id/org_function_Ll"
         android:orientation="vertical"
         android:background="#f1f1f1"
          android:layout_width="match_parent"
            android:layout_height="match_parent">
	 	 <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_finance_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="财政局"/>
	 	  <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_NDRC_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="发改委"/>
	 	   <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_SASAC_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="国资委"/>
	 	    <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_FB_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="金融局"/>
	 	    
	 	     <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_PB_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="规划局"/>
	 	      <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_CONUS_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="国土局"/>
	 	       <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_BB_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="商务局"/>
	 	       <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_BI_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="工信局"/>
	 	       <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_FIG_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="市建局"/>
	 	       <com.tr.ui.people.cread.view.MyitemView
                                    android:id="@+id/org_function_JB_Mv"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    edittextview:text_MyitemView_label="司法局"/>
	 
	 </LinearLayout>  
       </ScrollView>
</LinearLayout>
