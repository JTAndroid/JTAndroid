<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f1f1"
    android:orientation="vertical" xmlns:edittextview="http://schemas.android.com/apk/res/com.tr">
     <RelativeLayout 
	     android:layout_width="match_parent"
	     android:background="#22292c"
   		 android:layout_height="49dp">
   		  <RelativeLayout 
         android:id="@+id/quit_personal_Rl"
        android:padding="11dp"
         android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <ImageView 
       android:clickable="false"
       android:focusable="false"
         android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          android:background="@drawable/people_left_arrow"
        />
    </RelativeLayout>
    <TextView 
        android:id="@+id/personal_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="个人情况"
        android:layout_centerInParent="true"
        android:textSize="17sp"
        android:textColor="#ffffff"
        />

    
    <TextView 
        android:id="@+id/finish_personal_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:clickable="true"
        android:enabled="true"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:text="@string/_finish"
        android:onClick="finish"
        android:padding="10dp"
         android:textColor="#ffffff"
        android:textSize="13sp"
         />
	  
	</RelativeLayout>
    <ScrollView 
         android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout 
	    android:orientation="vertical"
	    android:background="#ffffff"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
<LinearLayout 
	    android:orientation="vertical"
	    android:background="#ffffff"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout 
	    android:orientation="horizontal"
	    android:background="#f1f1f1"
	    android:padding="10dp"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
	
	</LinearLayout>
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/nationality_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="国籍"  
        edittextview:isChoose="true"
        edittextview:isReadOnly="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/nation_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="民族" 
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/nativePlace_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="籍贯" 
        edittextview:isChoose="true" 
        edittextview:isReadOnly="true"
	    />
	
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/belief_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="信仰"  
        edittextview:isChoose="true"
        edittextview:isReadOnly="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/physicalCondition_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="身体情况"
        edittextview:isChoose="true"  
        edittextview:isReadOnly="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/hobby_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="爱好"  
        edittextview:isChoose="true"
        edittextview:isReadOnly="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/livingHabit_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="生活习惯"  
        edittextview:isChoose="true"
        edittextview:isReadOnly="true"
	    />
	<LinearLayout 
	    android:orientation="vertical"
	    android:background="#ffffff"
	    android:id="@+id/personal_addmore_ll"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/data_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="生日"  
  		 edittextview:isAddMore="true"
  		  edittextview:isPopupwindow_Text="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/socialRelations_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="配偶"  
  		 edittextview:isAddMore="true"
  		  edittextview:isPopupwindow_Text="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/custom_personal_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="自定义"  
        edittextview:isChoose="true"
  		 edittextview:isAddMore_hint="true"
	    />
	
	</LinearLayout>
	<include layout="@layout/people_include_delete"/>
	</LinearLayout>
	</LinearLayout>
	</ScrollView>
</LinearLayout>