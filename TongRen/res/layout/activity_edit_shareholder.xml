<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:edittextview="http://schemas.android.com/apk/res/com.tr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f1f1"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:background="#22292c" >

        <RelativeLayout
            android:id="@+id/editsave_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="11dp" >

            <ImageView
                
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:background="@drawable/people_left_arrow"
                android:clickable="true"
                android:focusable="true" />
        </RelativeLayout>

        <TextView
            android:id="@+id/shareholder_Tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="股东研究"
            android:textColor="#ffffff"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/edit_save"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:onClick="save"
            android:clickable="true"
            android:enabled="true"
            android:padding="10dp"
            android:text="保存"
            android:textColor="#ffffff" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#f1f1f1"
        android:orientation="vertical" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:text=""
            android:textColor="#1f1f1f"
            android:textSize="13sp"
            android:textStyle="bold" />
    </LinearLayout>
	<LinearLayout 
	    android:id="@+id/edit_Ll"
	    android:orientation="vertical"
	    android:layout_width="match_parent"
            android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="#ffffff"
        android:gravity="center_vertical"
         >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="控股股东"
            android:textColor="#000000"
            android:textSize="15sp" />
    </RelativeLayout>

    <View
        android:id="@+id/view_line"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#e3e3e3"
      />
		 <com.tr.ui.people.cread.view.MyEditTextView
                    android:id="@+id/controlling_shareholder_name_Etv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    edittextview:text_label="姓     名" />


    <com.tr.ui.people.cread.view.MyEditTextView
                    android:id="@+id/controlling_shareholder_ratio_Etv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    edittextview:text_label="持股比例" />
      <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="#ffffff"
        android:gravity="center_vertical"
         >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="实际控股人"
            android:textColor="#000000"
            android:textSize="15sp" />
    </RelativeLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#e3e3e3"
      />
 <com.tr.ui.people.cread.view.MyEditTextView
                    android:id="@+id/concert_party_name_Etv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    edittextview:text_label="姓     名" />


    <com.tr.ui.people.cread.view.MyEditTextView
                    android:id="@+id/concert_party_ratio_Etv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    edittextview:text_label="持股比例" />

     <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="#ffffff"
        android:gravity="center_vertical"
         >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="最终控股人"
            android:textColor="#000000"
            android:textSize="15sp" />
    </RelativeLayout>
 <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#e3e3e3"
      />
     <com.tr.ui.people.cread.view.MyEditTextView
                    android:id="@+id/Ultimate_controlling_people_name_Etv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    edittextview:text_label="姓     名" />


    <com.tr.ui.people.cread.view.MyEditTextView
                    android:id="@+id/Ultimate_controlling_people_ratio_Etv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    edittextview:text_label="持股比例" />
 
	<View 
	    android:background="#f1f1f1"
	    android:layout_width="match_parent"
                    android:layout_height="10dp"/>
	<com.tr.ui.people.cread.view.MyEditTextView
                    android:id="@+id/eidt_custom_Etv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    edittextview:isChoose="true"
                    edittextview:isAddMore_hint="true"
                    edittextview:text_label="自定义" />
	
	</LinearLayout>
</LinearLayout>