<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/common_margain_top_88" >
    <ImageView 
        android:id="@+id/common_image_empty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        />
    <TextView 
        android:id="@+id/common_text_empty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/common_image_empty"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/common_margain_top_29"
        android:textColor="@color/common_empty_text"
        android:textSize="@dimen/commen_text_size_14"/>

</RelativeLayout>
