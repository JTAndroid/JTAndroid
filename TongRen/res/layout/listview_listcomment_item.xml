<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout11"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/flow_bg"
    android:descendantFocusability="blocksDescendants"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/HFLLICommentName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_margain_top_4"
        android:layout_marginBottom="@dimen/common_margain_bottom_4"
        android:text=""
        android:textColor="@color/text_flow_more"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/HFLLICommentContent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_margain_top_4"
        android:layout_marginBottom="@dimen/common_margain_bottom_4"
        android:text=""
        android:textColor="@color/text_flow_address"
        android:textSize="12sp" />

</LinearLayout>