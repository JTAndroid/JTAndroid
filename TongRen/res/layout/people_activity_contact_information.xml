<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:edittextview="http://schemas.android.com/apk/res/com.tr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f1f1"
    android:orientation="vertical" >
     <RelativeLayout 
	     android:layout_width="match_parent"
	     android:background="#22292c"
   		 android:layout_height="49dp">
   		   <RelativeLayout 
   		       android:id="@+id/quit_contact_Rl"
        android:padding="11dp"
         android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <ImageView
        android:focusable="false"
        android:clickable="false"
         android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          android:background="@drawable/people_left_arrow"
        />
    </RelativeLayout>
    <TextView 
        android:id="@+id/contact_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="联系方式"
        android:layout_centerInParent="true"
        android:textSize="17sp"
        android:textColor="#ffffff"
        />

    
    <TextView 
        android:id="@+id/finish_contact_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:clickable="true"
        android:enabled="true"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:text="@string/_finish"
        android:padding="10dp"
        android:onClick="finish"
         android:textColor="#ffffff"
        android:textSize="13sp"
         />
	  
	</RelativeLayout>
	<ScrollView 
	     android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout 
        android:background="#ffffff"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout 
        android:id="@+id/contact_ll"
	    android:orientation="vertical"
	    android:background="#ffffff"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <View 
	    android:background="#f1f1f1"
	    android:layout_width="match_parent"
        android:layout_height="10dp">
	</View>
	
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/phone_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="手机"  
  		 edittextview:isAddMore="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/fixphone_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="固话"  
  		 edittextview:isAddMore="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/fox_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="住宅传真"  
  		 edittextview:isAddMore="true"
	    />
	
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/email_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="主要邮箱"  
  		 edittextview:isAddMore="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/homepage_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="个人主页"  
  		 edittextview:isAddMore="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/communication_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="QQ"  
  		 edittextview:isAddMore="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/address_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="住宅地址"  
  		 edittextview:isAddMore="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/custom_contact_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="自定义"
        edittextview:isAddMore="true"
        edittextview:isReadOnly="true"
	    />
	</LinearLayout>
	</LinearLayout>
</ScrollView>
</LinearLayout>