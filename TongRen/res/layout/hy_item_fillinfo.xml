<?xml version="1.0" encoding="utf-8"?>
<!-- create by roffee -->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView 
        android:id="@+id/hy_item_fillinfo_text"
       	android:layout_width="76dp"
   		android:layout_height="wrap_content"
   		android:padding="4dp"
   		android:layout_margin="2dp"
   		android:layout_gravity="center"
   		android:textColor="#f98512"
   		android:textSize="16sp"
   		android:text="所属部门"
   		android:singleLine="true"
   		android:ellipsize="end"
   		android:gravity="center"
   		android:background="@drawable/hy_shape_fillinfo_select_pressed"
   		android:clickable="true"/>
    <ImageView android:id="@+id/hy_item_fillinfo_deleteIcon"
        android:layout_width="8dp"
        android:layout_height="8dp"
        android:layout_gravity="top|right"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:src="@drawable/conns_item_delete_off"
        android:clickable="false"
        android:visibility="gone"/>
</FrameLayout>
