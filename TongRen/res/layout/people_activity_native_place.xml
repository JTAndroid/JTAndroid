<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f1f1"
    android:orientation="vertical" >
     <RelativeLayout 
	     android:layout_width="match_parent"
	     android:background="#22292c"
   		 android:layout_height="49dp">
   		 <RelativeLayout 
        android:id="@+id/quit_native_Rl"
        android:padding="11dp"
         android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <ImageView 
         android:layout_centerVertical="true"
         android:focusable="false"
         android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:background="@drawable/people_left_arrow"
        />
    	</RelativeLayout>
    <TextView 
        android:id="@+id/native_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="籍贯"
        android:layout_centerInParent="true"
        android:textSize="17sp"
        android:textColor="#ffffff"
        />
	<TextView 
        android:id="@+id/finish_native_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:clickable="true"
        android:enabled="true"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="10dp"
        android:text="@string/_finish"
        android:onClick="finish"
         android:textColor="#ffffff"
        android:textSize="13sp"
         />
    
	  
	</RelativeLayout>
	 <LinearLayout 
	    android:orientation="horizontal"
	    android:background="#f1f1f1"
	    android:padding="10dp"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
     </LinearLayout>
 
     <ListView 
         android:id="@+id/native_place_Lv"
         android:background="#ffffff"
          android:layout_width="match_parent"
        android:layout_height="wrap_content">
         
     </ListView>

</LinearLayout>