<?xml version="1.0" encoding="utf-8"?>
<!-- 20141103 zhang nian 
	 我的会议-资料页面布局  -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/hy_myhy_ll_archives"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/jt_list_style"
    android:orientation="vertical"
     >
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#e3e3e3">
		<TextView
			android:id="@+id/hy_myhy_archives_tv_year"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:layout_centerVertical="true"
			android:layout_marginRight="60px"
			android:layout_marginLeft="125px"
			android:text="2014年"
			android:textColor="#8d8d8d"
			android:textSize="50px" />
		<ImageView
			android:id="@+id/hy_myhy_archives_left_logo"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_toRightOf="@id/hy_myhy_archives_tv_year"
			android:layout_centerVertical="true"
			android:background="@drawable/hy_arrow_right" />

		<ImageView
			android:id="@+id/hy_myhy_archives_right_logo"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="30dp"
			android:layout_centerVertical="true"
			android:background="@drawable/hy_arrow_left" />
	</RelativeLayout>
	<ExpandableListView
	android:id="@+id/list"
	android:layout_width="fill_parent" 
	android:layout_height="wrap_content" 
	
	/>
   
</LinearLayout>