<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f1f1"
    android:orientation="vertical" xmlns:edittextview="http://schemas.android.com/apk/res/com.tr">
  
    <RelativeLayout 
	     android:layout_width="match_parent"
	     android:background="#22292c"
   		 android:layout_height="49dp">
   		  <RelativeLayout 
        android:id="@+id/quit_financing_Rl"
        android:padding="11dp"
         android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <ImageView
         android:layout_centerVertical="true"
         android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
         android:background="@drawable/people_left_arrow"
        />
    </RelativeLayout>
    <TextView 
        android:id="@+id/financing_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="融资意向"
        android:layout_centerInParent="true"
        android:textSize="17sp"
        android:textColor="#ffffff"
        />

    
    <TextView 
        android:id="@+id/finish_financing_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:clickable="true"
        android:enabled="true"
        android:padding="10dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:text="@string/_finish"
        android:onClick="finish"
         android:textColor="#ffffff"
        android:textSize="13sp"
         />
	  
	</RelativeLayout>
    <ScrollView 
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
<LinearLayout 
    android:id="@+id/financing_main_Ll"
    	android:background="#ffffff"
	    android:orientation="vertical"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
     <LinearLayout 
	    android:orientation="horizontal"
	    android:background="#f1f1f1"
	    android:padding="5dp"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
     </LinearLayout>
     <include layout="@layout/people_include_continueadd"/> 
	<LinearLayout 
    	android:id="@+id/financing_Ll"
   		 android:background="#ffffff"
	    android:orientation="vertical"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
  <LinearLayout 
	    android:orientation="horizontal"
	    android:background="#f1f1f1"
	    android:padding="5dp"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content">
        </LinearLayout>
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/area_financing_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="地区"  
        edittextview:isChoose="true"
        edittextview:isReadOnly="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/industry_financing_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="行业"  
        edittextview:isChoose="true"
        edittextview:isReadOnly="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/type_financing_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="类型"  
        edittextview:isChoose="true"
        edittextview:isReadOnly="true"
	    />
	<com.tr.ui.people.cread.view.MyEditTextView
	    android:id="@+id/custom_financing_Etv"
	    android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        edittextview:text_label="自定义"  
         edittextview:isChoose="true"
          edittextview:isReadOnly="true"
  		 edittextview:isAddMore_hint="true"
	    />
	<include layout="@layout/people_include_delete"/> 
	</LinearLayout>
	</LinearLayout>
	</ScrollView>
</LinearLayout>